{
    "bindings": [
      {
        "authLevel": "function",
        "name": "Request",
        "type": "httpTrigger",
        "direction": "in",
        "route": "suspendingOrchestrators/{FunctionName}",
        "methods": [
          "post",
          "get"        
        ]
      },
      {
        "type": "http",
        "direction": "out",
        "name": "Response"
      },
      {
        "name": "starter",
        "type": "durableClient",
        "direction": "in"
      }    
    ]
  }